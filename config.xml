<?xml version='1.0' encoding='utf-8'?>
<widget id="com.jiaojiaoxu.birdisko" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Birdisko</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
	<access origin="*" />

    <platform name="ios">
    <icon src="icons/Icon.png" width="120" height="120" />        
    <splash src="icons/Launch.png" width="750" height="1334"/>
	</platform>
    
    <preference name="DisallowOverscroll" value="true" />
    <preference name="UIWebViewBounce" value="false" />

</widget>
